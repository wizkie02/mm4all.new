{
  "name": "mm4all-admin-setup",
  "version": "1.0.0",
  "description": "MM4All Admin System Setup and Development Scripts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "admin:dev": "concurrently \"npm run dev\" \"npm run api:watch\"",
    "admin:build": "npm run lint && npm run build",
    "admin:test": "npm run test:admin",
    "test:admin": "jest src/test/admin --watchAll=false",
    "api:watch": "echo 'API server should be running on your web server'",
    "setup:admin": "node scripts/setup-admin.js",
    "db:migrate": "node scripts/migrate-db.js",
    "db:seed": "node scripts/seed-admin-data.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "styled-components": "^5.3.6",
    "framer-motion": "^8.5.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "concurrently": "^7.6.0"
  }
}
